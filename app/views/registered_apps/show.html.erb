<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="row">
      <div class="col-md-12">
        <h1><%= @registered_app.name %></h1>
      </div>
    </div>
    <div class="row">
      <div class="col-md-9">
        <h4><%= @registered_app.url %></h4>
      </div>
      <div class="col-md-3">
        <ul class="nav nav-pills">
          <li class="active"><%= link_to "Edit", edit_registered_app_path %></li>
        </ul>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <h1></h1>
</div>
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Events</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Number</th>
        </tr>
      </thead>
      <tbody>
        <% @events.each do |name, events| %>
          <tr>
            <td><%= name %></td>
            <td><%= events.count %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
