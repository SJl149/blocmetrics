<h1 class="text-center">Index of Apps</h1>
<div class="panel panel-default">
  <div class="panel-body">
    <%= render 'registered_apps/form' %>
  </div>
</div>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>URL</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody class="apps">
    <%= render @registered_apps %>
  </tbody>
</table>
